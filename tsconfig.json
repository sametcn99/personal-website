{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/components/game/tictactoe/TicTacToe.js",
    "src/app/components/header/header.s",
    "src/app/components/game/tictactoe/utils/utils.js",
    "src/app/helpers/gtm.js",
    "src/app/page.tsx",
    "src/app/(pages)/admin/login/page.tsx",
    "src/app/(pages)/admin/get-admin-key.js",
    "src/app/components/header/test_header.jsx",
    "src/app/lib/gtag-manager.js"
  ],
  "exclude": ["node_modules"]
}
